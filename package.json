{
	"name": "upload-here",
	"displayName": "upload here",
	"icon": "assets/icon.png",
	"description": "right click here, short cut here, upload here",
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.45.0"
	},
	"publisher": "sweetzone",
	"categories": [
		"Other"
	],
	"keywords": [
		"upload",
		"image",
		"insert",
		"flash"
	],
	"activationEvents": [
		"onCommand:upload-here.UploadHere"
	],
	"repository": "https://github.com/zjzhome/upload-here",
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "upload-here.UploadHere",
				"title": "upload here"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "upload-here.UploadHere",
					"alt": "upload-here.UploadHere",
					"group": "1_modification"
				}
			]
		},
		"configuration": {
			"type": "object",
			"title": "upload here configuration",
			"properties": {
				"upload here.uploadUrl": {
					"type": "string",
					"default": null,
					"description": "指定上传图片接口地址"
				},
				"upload here.tinyPNGKey": {
					"type": "string",
					"default": null,
					"description": "tinyPNG key，如果指定key则会先调用tinyPNG进行压缩"
				}
			}
		},
		"keybindings": [
			{
				"command": "upload-here.UploadHere",
				"key": "shift+alt+p",
				"mac": "shift+alt+p",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.45.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"axios": "^0.19.2",
		"form-data": "^3.0.0",
		"tinify": "^1.6.0-beta.2"
	}
}
